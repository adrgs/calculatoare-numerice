#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x561e1106ad10 .scope module, "Testbench" "Testbench" 2 56;
 .timescale -9 -12;
v0x561e1108b370_0 .var "A_t", 0 0;
v0x561e1108b440_0 .var "B_t", 0 0;
v0x561e1108b510_0 .net "D_t", 3 0, v0x561e1108b230_0;  1 drivers
S_0x561e1106ae90 .scope module, "Decoder_1" "Decoder" 2 61, 2 32 0, S_0x561e1106ad10;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "A"
    .port_info 1 /INPUT 1 "B"
    .port_info 2 /OUTPUT 4 "D"
v0x561e1106b0b0_0 .net "A", 0 0, v0x561e1108b370_0;  1 drivers
v0x561e1108b170_0 .net "B", 0 0, v0x561e1108b440_0;  1 drivers
v0x561e1108b230_0 .var "D", 3 0;
E_0x561e1102eb50 .event edge, v0x561e1108b170_0, v0x561e1106b0b0_0;
    .scope S_0x561e1106ae90;
T_0 ;
    %wait E_0x561e1102eb50;
    %load/vec4 v0x561e1106b0b0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x561e1108b170_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x561e1108b230_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x561e1106b0b0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x561e1108b170_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 2, 0, 4;
    %assign/vec4 v0x561e1108b230_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x561e1106b0b0_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x561e1108b170_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %pushi/vec4 4, 0, 4;
    %assign/vec4 v0x561e1108b230_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x561e1108b230_0, 0;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x561e1106ad10;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561e1108b370_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561e1108b440_0, 0;
    %delay 1000, 0;
    %vpi_call 2 68 "$display", "D_t = %b", v0x561e1108b510_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561e1108b370_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561e1108b440_0, 0;
    %delay 1000, 0;
    %vpi_call 2 72 "$display", "D_t = %b", v0x561e1108b510_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561e1108b370_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561e1108b440_0, 0;
    %delay 1000, 0;
    %vpi_call 2 76 "$display", "D_t = %b", v0x561e1108b510_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561e1108b370_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561e1108b440_0, 0;
    %delay 1000, 0;
    %vpi_call 2 80 "$display", "D_t = %b", v0x561e1108b510_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
